{
  "name": "insight-mon",
  "version": "5.2.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "prebuild": "rm -rf ./src/.umi-production",
    "build": "max build",
    "dev": "npm run start:dev",
    "dev:local": "npm run start:local",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "mock": "cross-env UMI_ENV=dev max dev",
    "openapi": "max openapi",
    "playwright": "playwright install && playwright test",
    "prettier": "prettier -c --write \"src/**/*\"",
    "prepare": "chmod +x ./node_modules/husky/lib/bin.js && husky install",
    "pre-commit": "pretty-quick --staged",
    "start": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:local": "cross-env REACT_APP_ENV=local MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "max test",
    "test:component": "max test ./src/components",
    "test:e2e": "node ./tests/run-tests.js",
    "temp": "max plugin list",
    "tsc": "tsc --noEmit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.4.3",
    "@ant-design/icons": "^4.8.3",
    "@antv/s2": "^1.54.8",
    "@antv/s2-react": "^1.46.4",
    "@types/is-hotkey": "^0.1.10",
    "@types/nanoid-dictionary": "^4.2.3",
    "@types/prismjs": "^1.26.3",
    "@types/react-infinite-scroller": "^1.2.5",
    "ahooks": "^3.7.11",
    "antd": "^4.24.16",
    "aws-sdk": "^2.1591.0",
    "china-division": "^2.7.0",
    "classnames": "^2.5.1",
    "convert": "^4.14.1",
    "deep-object-diff": "^1.1.9",
    "hox": "^2.1.1",
    "is-hotkey": "^0.2.0",
    "lodash": "^4.17.21",
    "lodash-decorators": "^6.0.1",
    "moment": "^2.30.1",
    "nanoid": "^4.0.2",
    "nanoid-dictionary": "^4.3.0",
    "numeral": "^2.0.6",
    "postcss-import": "^15.1.0",
    "postcss-url": "^10.1.3",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "query-string": "^8.2.0",
    "rc-menu": "^9.13.0",
    "rc-tween-one": "^3.0.6",
    "rc-util": "^5.39.1",
    "react": "^17.0.2",
    "react-css-theme-switcher": "^0.3.0",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.4.4",
    "react-infinite-scroller": "^1.2.6",
    "react-markdown": "^8.0.7",
    "react-redux": "^7.2.9",
    "react-router": "^4.3.1",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1",
    "slate": "~0.94.1",
    "slate-history": "~0.93.0",
    "slate-hyperscript": "~0.77.0",
    "slate-react": "~0.94.2",
    "staged-components": "^1.1.3",
    "styled-components": "^5.3.11",
    "swr": "^2.2.5",
    "umi-request": "^1.4.0",
    "use-async-memo": "^1.2.5",
    "with-wrapper": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@playwright/test": "^1.42.1",
    "@types/express": "^4.17.21",
    "@types/history": "^4.7.11",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.17.0",
    "@types/react": "^17.0.80",
    "@types/react-dom": "^17.0.25",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-helmet": "^6.1.11",
    "@types/styled-components": "^5.1.34",
    "@umijs/fabric": "^3.0.0",
    "@umijs/max": "^4.1.7",
    "@umijs/max-plugin-openapi": "^2.0.3",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.3.1",
    "stylelint": "^15.11.0",
    "swagger-ui-react": "^4.19.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
